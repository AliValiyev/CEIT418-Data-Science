tl = [15, 3, 1, None, 9, 20, None, 25, 56, 14, 90, None, 0, 80, 96, 36, None]



def CountNoneValues(x: list):

  count = 0

  for i in x:

    if i == None:

      count += 1

  return count



CountNoneValues(tl)



def SumofNumbers(x: list):

  total = 0

  for i in x:

    if type(i) == int:

      total += i

  return total



SumofNumbers(tl)



def CountifBigger(k: int, x: list):

  count = 0

  for i in x:

    if type(i) == int:

      if i > k:

        count += 1

  return count



CountifBigger(20, tl)



def CompareOddEvenNumbers(x: list):

  count_odd, count_even = 0, 0

  for i in x:

    if type(i) == int:

      if i%2 == 0:

        count_even += 1

      else:

        count_odd += 1

  if count_even > count_odd:

    print("There are more even numbers than odd numbers.")

  elif count_odd > count_even:

    print("There are more odd numbers than even numbers.")

  else:

    print("There are equal amount of even and odd numbers.")



CompareOddEvenNumbers(tl)
